def minimum_masks_needed(test_cases):
    results = []
    for case in test_cases:
        N, A = case
        
        results.append(A)
    return results


T = int(input())
test_cases = []


for _ in range(T):
    N, A = map(int, input().split())
    test_cases.append((N, A))


results = minimum_masks_needed(test_cases)


for result in results:
    print(result)
