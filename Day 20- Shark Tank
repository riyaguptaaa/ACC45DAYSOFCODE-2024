def evaluate_offers(A, B):
    valuation_1 = 10 * A 
    valuation_2 = 5 * B    

    if valuation_1 > valuation_2:
        return "Accept the first investor's offer."
    elif valuation_1 < valuation_2:
        return "Accept the second investor's offer."
    else:
        return "Both offers are equally good."

A = float(input("Enter the amount offered by the first investor (A): "))
B = float(input("Enter the amount offered by the second investor (B): "))
result = evaluate_offers(A, B)
print(result)
